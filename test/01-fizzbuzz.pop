int i = 1
int limit = 100

void print_fizz(){
    print_char('F')
    print_char('i')
    print_char('z')
    print_char('z')
}

void print_buzz(){
    print_char('B')
    print_char('u')
    print_char('z')
    print_char('z')
}

while (i < limit) {
    if (i % 3 == 0) print_fizz()
    if(i % 5 == 0) print_buzz()
    if(i % 3 == 0) print_char('\n')
    else if(i % 5 == 0) print_char('\n')
    else print_int(i)
    i = i + 1
}

